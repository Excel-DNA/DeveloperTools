<?xml version="1.0" encoding="utf-8"?>
<VSTemplate Version="3.0.0" Type="Item" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" xmlns:sdk="http://schemas.microsoft.com/developer/vstemplate-sdkextension/2010">
  <TemplateData>
    <Name>Excel-DNA Ribbon</Name>
    <Description>An Excel ribbon extension.</Description>
    <Icon>Logo.ico</Icon>
    <TemplateID>86149c5e-3406-4c9b-ba3e-936dd67d0ec4</TemplateID>
    <ProjectType>CSharp</ProjectType>
    <RequiredFrameworkVersion>4.5</RequiredFrameworkVersion>
    <NumberOfParentCategoriesToRollUp>1</NumberOfParentCategoriesToRollUp>
    <DefaultName>Ribbon.cs</DefaultName>
    <TemplateGroupID>Excel-DNA Add-in</TemplateGroupID>
    <ShowByDefault>false</ShowByDefault>
  </TemplateData>
  <TemplateContent>
    <References>
      <Reference>
        <Assembly>System.Windows.Forms</Assembly>
        <Assembly>System.Drawing</Assembly>
      </Reference>
    </References>
    <ProjectItem ReplaceParameters="true" TargetFileName="$fileinputname$.cs">Ribbon.cs</ProjectItem>
    <ProjectItem ReplaceParameters="true" TargetFileName="$fileinputname$Resources.resx" CustomTool="ResXFileCodeGenerator">RibbonResources.resx</ProjectItem>
    <ProjectItem ReplaceParameters="true" TargetFileName="$fileinputname$Resources\Ribbon.xml">Ribbon.xml</ProjectItem>
    <ProjectItem ReplaceParameters="true" TargetFileName="$fileinputname$Resources\Image1.png">Image1.png</ProjectItem>
  </TemplateContent>
</VSTemplate>